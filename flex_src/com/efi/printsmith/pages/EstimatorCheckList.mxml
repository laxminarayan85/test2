				dataGridItem = new Object();
				dataGridItem.item = resourceManager.getString(Snowmass.RESOURCE_BUNDLE,'kEstimatorChecklistCommand.ColorsBack');
				dataGridList.addItem(dataGridItem);
				dataGridItem = new Object();

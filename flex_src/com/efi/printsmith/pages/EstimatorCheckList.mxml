			private function getTime(milliseconds:Number):String {
				if(!isNaN()) {
					return convertMilliSecondsToString(milliseconds,true);
				}
				return resourceManager.getString(Snowmass.RESOURCE_BUNDLE,'kEstimatorChecklistCommand.NA');
			}
		]]>

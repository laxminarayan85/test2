						enabled="{trackerConsoleJobs.job!=null &amp;&amp; !trackerConsoleJobs.treatAsNonProdStep}" disabledColor="#000000"
						focusOut="{setSpoilageValue('end')}"/>
					
				</mx:HBox>
				

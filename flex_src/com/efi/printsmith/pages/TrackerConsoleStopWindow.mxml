					customText="{getOrderedQty(trackerConsoleJobs,trackerConsoleJobs.treatAsNonProdStep)}" textAlignStyle="left"
					precision="0" rounding="nearest" isInteger="true" width="30%" enabled="false" disabledColor="#000000"/>

	private function handleBrickRead(ev:CreditCardTrackReadEvent):void 
	{
		var valid:Boolean;
		var returnVal:Array = new Array;
		
		contact.creditCard.cardNumber = ev.brickData.parsedCard.cardNumber;
		contact.creditCard.cardHolderName = ev.brickData.parsedCard.cardHolderName;
		
		contact.creditCard.expiresDate = ev.brickData.parsedCard.expiresDate;
		cardexpiresdate.selectedDate = contact.creditCard.expiresDate;
		
		// following two must be in this order, the formatCreditCardNumber will zero out the cardNumber
		returnVal = validCreditCard(contact.creditCard);
		contact.creditCard.cardDisplayNumber = formatCreditCardNumber(ev.brickData.parsedCard.cardNumber);
		
		valid = returnVal[0] as Boolean;
	//	cardstatus.text = returnVal[1] as String;
		if (valid == true) {
			contact.creditCard.type = returnVal[1] as String;
			cardtype.text = returnVal[1] as String;
			cardtype.selectedIndex = findItem(cardtype.text, creditCardPreferencesArray);
		}
	}


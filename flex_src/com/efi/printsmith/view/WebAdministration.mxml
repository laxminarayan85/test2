	}
	private function handleSaveGraphicsResult(ev:ResultEvent):void {
		webgraphics = ev.result as WebGraphics;
	}
	private function handleSaveProductsResult(ev:ResultEvent):void {
		webproducts = ev.result as WebProducts;
	}
	private function handleSaveQuestionsResult(ev:ResultEvent):void {
		webquestions = ev.result as WebQuestions;
	}
	private function handleSaveStatusResult(ev:ResultEvent):void {
		webstatus = ev.result as WebStatus;
	}

	private function handleFault(ev:FaultEvent):void {  
		var message:String;
		              
		message = "Error: "                          
		+ ev.fault.faultCode + " - "                                  
		+ ev.fault.faultDetail + " - "                                  
		+ ev.fault.faultString;
		Alert.show(message, "Error");                                 
	}
	    

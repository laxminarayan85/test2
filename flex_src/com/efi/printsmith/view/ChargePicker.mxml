			private function treeLabel(item:Object):String
			{
				var children:ICollectionView;

				var suffix:String="";
				if (chargeTree.dataDescriptor.isBranch(item))
				{
					children=chargeTree.dataDescriptor.getChildren(item);
					suffix=" (" + children.length + ")";
				}
				return item.name + suffix;
			}

			private function updateCurrentState():void {
				if (chargeDefinition.method == ChargeMethod.SQUAREAREA) {
					this.currentState = "SquareArea";
				} else if (chargeDefinition.method == ChargeMethod.CUT) {
					this.currentState = "CuttingCharge";
				} else if (chargeDefinition.method == ChargeMethod.INK) {
					this.currentState = "InkCharge";
				} else if (chargeDefinition.method == ChargeMethod.SHIPPING) {
					this.currentState = "ShippingCharge";
				} else if (chargeDefinition.method == ChargeMethod.ASK) {
					if (chargeDefinition.quantityType == ChargeQtyType.NONE) {
						this.currentState = "AlwaysAsk1";
					} else if (chargeDefinition.quantityType == ChargeQtyType.QUANTITY) {
						this.currentState = "AlwaysAsk2";
					} else if (chargeDefinition.quantityType == ChargeQtyType.SETS) {
						this.currentState = "AlwaysAsk4";
					} else if (chargeDefinition.quantityType == ChargeQtyType.SETUPSETS) {
						this.currentState = "AlwaysAsk4";
					} else if (chargeDefinition.quantityType == ChargeQtyType.TIME) {
						this.currentState = "AlwaysAsk3";
					}
				} else if (chargeDefinition.method == ChargeMethod.FLATRATE) {
					this.currentState = "FlatRate";
				} else if (chargeDefinition.method == ChargeMethod.FOLD) {
					this.currentState = "Fold";
				} else if (chargeDefinition.method == ChargeMethod.JOBAWARE) {
					this.currentState = "JobAware";
				} else if (chargeDefinition.method == ChargeMethod.LINEAR) {
					this.currentState = "Linear";
				} else if (chargeDefinition.method == ChargeMethod.MARKUP) {
					this.currentState = "Markup";
				} else if (chargeDefinition.method == ChargeMethod.PRICELIST) {
					this.currentState = "PriceList";
				} else if (chargeDefinition.method == ChargeMethod.RATELIST) {
					this.currentState = "RateList";
				}			
			}
			private function handleNewChargeResult(event:ResultEvent):void
			{
				if (chargeDefinition.method == ChargeMethod.SQUAREAREA) {
					charge = event.result as SquareAreaCharge;
				} else if (chargeDefinition.method == ChargeMethod.CUT) {
					charge = event.result as CuttingCharge;
				} else if (chargeDefinition.method == ChargeMethod.INK) {
					charge = event.result as InkCharge;
				} else if (chargeDefinition.method == ChargeMethod.SHIPPING) {
					charge = event.result as ShippingCharge;
				} else {
					charge = event.result as Charge;
				}
				charge.parentJob = job;
				updateCurrentState();
				updatePrice();
			}
			

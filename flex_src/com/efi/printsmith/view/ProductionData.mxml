	private function setTimes():void {
		var setupHours:Number = Math.floor(job.setupTime);
		var setupHoursString:String = "";
		if (setupHours >= 1)
			setupHoursString = setupHours.toFixed(0).replace(".","")
		else
			setupHoursString = "0";
		var setupMinutes:Number = (job.setupTime - Math.floor(job.setupTime)) * 60;
		if (setupMinutes < 10)
			this.setup_time.text = setupHoursString + ":0" + setupMinutes.toFixed(0).replace(".","");
		else
			this.setup_time.text = setupHoursString + ":" + setupMinutes.toFixed(0).replace(".","");
		var minimumHours:Number = this.job.pricingPress.minLabor / 60;
		var runHours:Number = Math.floor(job.runTime);
		if (job.orRuntime == false && runHours < minimumHours)
			runHours = minimumHours;
		if (job.orRuntime == false && job.doubleSided == true)
			runHours = runHours * 2;
		var runHoursString:String = "";
		if (runHours >= 1)
			runHoursString = runHours.toFixed(0).replace(".","");
		else
			runHoursString = "0";
		var runMinutes:Number = (job.runTime - Math.floor(job.runTime)) * 60;
		if (runMinutes < 10)
			this.run_time.text = runHoursString + ":0" + runMinutes.toFixed(0).replace(".","");
		else
			this.run_time.text = runHoursString + ":" + runMinutes.toFixed(0).replace(".","");
		var washupHours:Number = Math.floor(job.washupTime / 60);
		var washupHoursString:String = "";
		if (washupHours >= 1)
			washupHoursString = washupHours.toFixed(0);
		else
			washupHoursString = "0";
		var washupMinutes:Number = job.washupTime - (parseInt(washupHoursString) * 60);
		if (washupMinutes < 10)
			this.washup_time.text = washupHoursString + ":0" + washupMinutes.toString().replace(".","");
		else
			this.washup_time.text = washupHoursString + ":" + washupMinutes.toString().replace(".","");
		var totalHours:Number = (parseInt(setupHoursString) + parseInt(runHoursString) + parseInt(washupHoursString));
		var totalMinutes:Number = setupMinutes + runMinutes + washupMinutes;
		var totalHoursString:String = "";
		if (totalHours >= 1)
			totalHoursString = totalHours.toFixed(0);
		else
			totalHoursString = "0";
		if (totalMinutes < 10)
			this.total_time.text = totalHoursString.replace(".","") + ":0" + totalMinutes.toFixed(0).replace(".","");
		else
			this.total_time.text = totalHoursString.replace(".","") + ":" + totalMinutes.toFixed(0).replace(".","");
	}
	

	private function doProductionPriceChange():void {
		if (this.production.text == "" || isNaN(parseInt(this.production.text))) {
			job.pricingRecord.productionPriceOverride = false;
			job.pricingRecord.productionPrice = 0.00;
			production.text = "0.00";
		}
		else
			job.pricingRecord.productionPriceOverride = true;
		this.setOverrideForField(production,job.pricingRecord.productionPriceOverride);
	}
	
	private function doCopierSpeedChange():void {
		if (this.copier_speed.text == "" || isNaN(parseInt(this.copier_speed.text)))
			job.copyMinutesOverride = false;
		else
			job.copyMinutesOverride = true;
		setOverrideForField(copier_speed,job.copyMinutesOverride);
	}
	
	private function doParentCostChange():void {
		if (this.parent_costs.text == "" || isNaN(parseInt(this.copier_speed.text)))
			job.parentStockCostOverride = false;
		else
			job.parentStockCostOverride = true;
		setOverrideForField(parent_costs,job.parentStockCostOverride);
	}
	
	private function doPerChange():void {
		if (this.per.text == "" || isNaN(parseInt(this.per.text)))
			job.costUnitsOverride = false;
		else
			job.costUnitsOverride = true;
		setOverrideForField(per,job.costUnitsOverride);
	}
	
	private function doMarkupChange():void {
		if (this.markup.text == "" || isNaN(parseInt(this.markup.text)))
			job.orStockMarkup = false;
		else
			job.orStockMarkup = true;
		setOverrideForField(markup,job.orStockMarkup);
	}
	
	private function doLaborRateChange():void {
		if (this.labor_rate.text == "" || isNaN(parseInt(this.labor_rate.text)))
			job.pricingRecord.laborRateOverride = false;
		else
			job.pricingRecord.laborRateOverride = true;
		setOverrideForField(labor_rate,job.pricingRecord.laborRateOverride);
	}
	
	private function doLaborMarkupChange():void {
		if (this.labor_rate_markup.text == "" || isNaN(parseInt(this.labor_rate_markup.text)))
			job.pricingRecord.laborMarkupOverride = false;
		else
			job.pricingRecord.laborMarkupOverride = true;
		setOverrideForField(labor_rate_markup,job.pricingRecord.laborMarkupOverride);
	}
	
	private function setAllOverrides():void {
		setOverrideForField(markup,job.orStockMarkup);
		setOverrideForField(per,job.costUnitsOverride);
		setOverrideForField(parent_costs,job.parentStockCostOverride);
		setOverrideForField(copier_speed,job.copyMinutesOverride);
		setOverrideForField(production,job.pricingRecord.productionPriceOverride);
		setOverrideForField(number_out,job.orRunOut);
		setOverrideForField(labor_rate,job.pricingRecord.laborRateOverride);
		setOverrideForField(labor_rate_markup,job.pricingRecord.laborMarkupOverride);
	}
	
	private function fieldChanged(evt:FocusEvent):void
	{	
		var doUpdatePrice:Boolean = false;
		
		switch (evt.currentTarget) {
			case this.number_out:
				job.paperCal.runout = parseInt(this.number_out.text);
				doUpdatePrice = true;
				break;
			case this.parent_costs:
				job.parentStockCost = parseFloat(this.parent_costs.text);
				doUpdatePrice = true;
				break;
			case this.per:
				job.costUnits = parseInt(this.per.text);
				doUpdatePrice = true;
				break;
			case this.copier_speed:
				job.copyMinutes = parseFloat(this.copier_speed.text);
				doUpdatePrice = true;
				break;
			case this.markup:
				job.pricingRecord.stockMarkup = parseFloat(this.markup.text);
				doUpdatePrice = true;
				break;
			case this.production:
				job.pricingRecord.productionPrice = parseFloat(this.production.text);
				doUpdatePrice = true;
				break;
			case this.labor_rate:
				job.pricingRecord.laborRate = parseFloat(this.labor_rate.text);
				doUpdatePrice = true;
				break;
			case this.labor_rate_markup:
				job.pricingRecord.laborMarkup = parseFloat(this.labor_rate_markup.text);
				doUpdatePrice = true;
				break;
			case this.setup_time:
				var hours:Number = 0;
				var minutes:Number = 0;
				if (this.setup_time.text.indexOf(":") != -1) {
					hours = parseInt(this.setup_time.text.substr(0,this.setup_time.text.indexOf(":")));
					minutes = parseInt(this.setup_time.text.substr(this.setup_time.text.indexOf(":")+1));
				} if (this.setup_time.text.indexOf(".") != -1) {
					hours = parseInt(this.setup_time.text.substr(0,this.setup_time.text.indexOf(".")));
					minutes = parseInt(this.setup_time.text.substr(this.setup_time.text.indexOf(".")+1));
				} if (this.setup_time.text.indexOf("/") != -1) {
					hours = parseInt(this.setup_time.text.substr(0,this.setup_time.text.indexOf("/")));
					minutes = parseInt(this.setup_time.text.substr(this.setup_time.text.indexOf("/")+1));
				} else {
					hours = parseInt(this.setup_time.text);
				}
				if (isNaN(hours))
					hours = 0;
				job.setupTime = ((hours * 60) + minutes) / 60;
				doUpdatePrice = true;
				break;
			case this.run_time:
				var hours:Number = 0;
				var minutes:Number = 0;
				if (this.run_time.text.indexOf(":") != -1) {
					hours = parseInt(this.run_time.text.substr(0,this.run_time.text.indexOf(":")));
					minutes = parseInt(this.run_time.text.substr(this.run_time.text.indexOf(":")+1));
				} if (this.run_time.text.indexOf(".") != -1) {
					hours = parseInt(this.run_time.text.substr(0,this.run_time.text.indexOf(".")));
					minutes = parseInt(this.run_time.text.substr(this.run_time.text.indexOf(".")+1));
				}if (this.run_time.text.indexOf("/") != -1) {
					hours = parseInt(this.run_time.text.substr(0,this.run_time.text.indexOf("/")));
					minutes = parseInt(this.run_time.text.substr(this.run_time.text.indexOf("/")+1));
				} else {
					hours = parseInt(this.run_time.text);
				}
				if (isNaN(hours))
					hours = 0;
				job.runTime = ((hours * 60) + minutes) / 60;
				doUpdatePrice = true;
				break;
			case this.washup_time:
				var hours:Number = 0;
				var minutes:Number = 0;
				if (this.washup_time.text.indexOf(":") != -1) {
					hours = parseInt(this.washup_time.text.substr(0,this.washup_time.text.indexOf(":")));
					minutes = parseInt(this.washup_time.text.substr(this.washup_time.text.indexOf(":")+1));
				} if (this.washup_time.text.indexOf(".") != -1) {
					hours = parseInt(this.washup_time.text.substr(0,this.washup_time.text.indexOf(".")));
					minutes = parseInt(this.washup_time.text.substr(this.washup_time.text.indexOf(".")+1));
				}if (this.washup_time.text.indexOf("/") != -1) {
					hours = parseInt(this.washup_time.text.substr(0,this.washup_time.text.indexOf("/")));
					minutes = parseInt(this.washup_time.text.substr(this.washup_time.text.indexOf("/")+1));
				} else {
					hours = parseInt(this.washup_time.text);
				}
				if (isNaN(hours))
					hours = 0;
				job.washupTime = ((hours * 60) + minutes);
				doUpdatePrice = true;
				break;
		}
		
		if (doUpdatePrice)
			this.updatePrice();
		
	}
	
	private function updatePrice():void
	{
		updateDates();
		var priceJobEvent:PriceJobEvent = new PriceJobEvent(this.job, new mx.rpc.Responder(handlePriceJobComplete, handlePriceJobFault));
		priceJobEvent.dispatch();
	}
	
	private function updateDates():void {
		job.typeSettingDate = this.typeSettingDate.selectedDate;
		job.proofBackDate = this.proofBackDate.selectedDate;
		job.bluelineDate = this.bluelineDate.selectedDate;
		job.bluelineBackDate = this.bluelineBackDate.selectedDate;
		job.stockCutDate = this.stockCutDate.selectedDate;
		job.atPressDate = this.atPressDate.selectedDate;
		job.printedDate = this.printedDate.selectedDate;
		job.copiedDate = this.copiedDate.selectedDate;
		job.binderyDate = this.binderyDate.selectedDate;
		job.cutDate = this.cutDate.selectedDate;
		job.foldedDate = this.foldedDate.selectedDate;
		job.finishedDate = this.foldedDate.selectedDate;
		job.mailingDate = this.mailingDate.selectedDate;
		job.outsideServiceDate = this.outsideServiceDate.selectedDate;
		job.otherDate = this.otherDate.selectedDate;
		job.deliveredDate = this.deliveredDate.selectedDate;
		job.runByDate = this.runByDate.selectedDate;
		job.startByDate = this.startByDate.selectedDate;
		job.submittedDate = this.submittedDate.selectedDate;
	}
	
	private function handlePriceJobComplete(ev:ResultEvent):void
	{
		this.job = ev.result as Job;
		this.setProductionData();
	}
	
	private function handlePriceJobFault(ev:FaultEvent):void
	{
		if (ev != null && ev.fault != null && ev.fault.faultString != null) {				
			Alert.show(ev.fault.faultString, "Error in Pricing", 0, null);
		} else {
			Alert.show("Unknown error when pricing job", "Error in Pricing", 0, null);
		}
	}
	
	private function doOK():void
	{
		updateDates();
		var selectEvent:JobEditCompleteEvent = new JobEditCompleteEvent(JobEditCompleteEvent.SAVE, job);
		dispatchEvent(selectEvent);
		var mdiWindow:MDIWindow = this.parent as MDIWindow;
		mdiWindow.closeWindow();
	}

	private function doCancel():void
	{
		var mdiWindow:MDIWindow = this.parent as MDIWindow;
		mdiWindow.closeWindow();
	}

				formatObj.id = "firstName";
				formatObj.name= "By Name";
				formatList.addItem(formatObj);
				formatObj = new Object();
				formatObj.id = "socialSecurity";
				formatObj.name= "By SSN";
				formatList.addItem(formatObj);

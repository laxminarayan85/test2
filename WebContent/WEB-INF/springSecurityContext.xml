<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:security="http://www.springframework.org/schema/security"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/security
		http://www.springframework.org/schema/security/spring-security-2.0.4.xsd">
	<security:http auto-config="true"> 
		<security:intercept-url pattern="index.html" filters="none"/>
		<security:intercept-url pattern="*.swf" filters="none"/>
		<security:intercept-url pattern="*.html" access="ROLE_USER"/>
	</security:http>
	
	<security:authentication-provider user-service-ref="authenticationService"/>

<!--	<bean id="authenticationService" class="com.efi.printsmith.security.AuthenticationService"/> -->
</beans>